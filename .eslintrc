{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "arrow-body-style": "off",
    "camelcase": "off",
    "consistent-return": "off",
    "global-require": "off",
    "import/no-dynamic-require": "off",
    "max-classes-per-file": "off",
    "no-param-reassign": "off",
    "no-underscore-dangle": "off",
    "object-curly-newline": "off"
  }
}
